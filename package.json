{
  "name": "rr-boilerplate",
  "version": "1.0.0",
  "description": "A React Redux Boilerplate.",
  "scripts": {
    "start": "npm run dev",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src src-example private webpack.config.js",
    "dev": "npm run env:dev -- webpack-dev-server",
    "build:clean": "rimraf dist",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "npm run env:prod -- webpack"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "src-example",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(sass|css|scss|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/jest/fileMock.js",
      "^components$": "<rootDir>/private/jest/componentsMock.js"
    },
    "setupFiles": [
      "<rootDir>/private/jest/setupTests.js"
    ]
  },
  "devDependencies": {
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.0.1",
    "expect": "^23.1.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^3.2.0",
    "jest-cli": "^22.1.4",
    "opn-cli": "^3.1.0",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-blocks-happypack": "^0.1.3",
    "webpack-blocks-split-vendor": "^0.2.0",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "@webpack-blocks/extract-text": "^1.0.0",
    "@webpack-blocks/sass": "^1.0.0",
    "@webpack-blocks/webpack": "^1.0.0",
    "axios": "^0.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "history": "^4.6.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.9.0",
    "prop-types": "^15.5.8",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.1.1",
    "redux": "^4.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "styled-components": "^3.0.2",
    "styled-theme": "^0.3.0",
    "styled-tools": "^0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lipeprado/rr-prod.git"
  },
  "keywords": [
    "React",
    "Redux",
    "Boilerplate",
    "styled-componets",
    "thunk",
    "eslint",
    "webpack"
  ],
  "author": "Filipe Prado",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lipeprado/rr-prod/issues"
  },
  "homepage": "https://github.com/lipeprado/rr-prod#readme"
}
